<?xml version="1.0"?>


<faces-config
	xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
version="1.2">

	<render-kit>
	     <renderer>
	          <component-family>webface</component-family>
	          <renderer-type>wf_container</renderer-type>
	          <renderer-class>com.idega.webface.renderkit.ContainerRenderer</renderer-class>
	     </renderer>
	     <renderer>
	          <component-family>webface</component-family>
	          <renderer-type>wf_bezel</renderer-type>
	          <renderer-class>com.idega.webface.renderkit.BezelRenderer</renderer-class>
	     </renderer>
	     <renderer>
	          <component-family>webface</component-family>
	          <renderer-type>wf_menu</renderer-type>
	          <renderer-class>com.idega.webface.renderkit.MenuRenderer</renderer-class>
	     </renderer>
	     <renderer>
	          <component-family>webface</component-family>
	          <renderer-type>wf_block</renderer-type>
	          <renderer-class>com.idega.webface.renderkit.BlockRenderer</renderer-class>
	     </renderer>
	     <renderer>
	          <component-family>webface</component-family>
	          <renderer-type>wf_titlebar</renderer-type>
	          <renderer-class>com.idega.webface.renderkit.TitlebarRenderer</renderer-class>
	     </renderer>
	     <renderer>
	          <component-family>webface</component-family>
	          <renderer-type>wf_toolbar</renderer-type>
	          <renderer-class>com.idega.webface.renderkit.ToolbarRenderer</renderer-class>
	     </renderer>
	     <renderer>
	          <component-family>webface</component-family>
	          <renderer-type>wf_htmlarea</renderer-type>
	          <renderer-class>com.idega.webface.htmlarea.HTMLAreaRenderer</renderer-class>
	     </renderer>
	    	<renderer>
	          <component-family>javax.faces.Data</component-family>
	          <renderer-type>wf_list</renderer-type>
	          <renderer-class>com.idega.webface.renderkit.ListRenderer</renderer-class>
	     </renderer>
	</render-kit>

	<component>
		<component-type>WFBlock</component-type>
		<component-class>com.idega.webface.WFBlock</component-class>
	</component>
	
	<component>
		<component-type>WFSimpleDocumentsView</component-type>
		<component-class>com.idega.webface.WFSimpleDocumentsView</component-class>
	</component>
	
	<component>
		<component-type>WFWebView</component-type>
		<component-class>com.idega.webface.WFWebView</component-class>
	</component>	

	<component>
		<component-type>HTMLArea</component-type>
		<component-class>com.idega.webface.htmlarea.HTMLArea</component-class>
	</component>
	
	<component>
		<component-type>WFContainer</component-type>
		<component-class>com.idega.webface.WFContainer</component-class>
	</component>
	
	<component>
		<component-type>HTMLAreaLinkCreator</component-type>
		<component-class>com.idega.webface.htmlarea.HTMLAreaLinkCreator</component-class>
	</component>

	<component>
		<component-type>HTMLAreaImageChooser</component-type>
		<component-class>com.idega.webface.htmlarea.HTMLAreaImageChooser</component-class>
	</component>
	
	<component>
		<component-type>WFToolbar</component-type>
		<component-class>com.idega.webface.WFToolbar</component-class>
	</component>
	
	<component>
		<component-type>WFToolbarButton</component-type>
		<component-class>com.idega.webface.WFToolbarButton</component-class>
	</component>

</faces-config>
